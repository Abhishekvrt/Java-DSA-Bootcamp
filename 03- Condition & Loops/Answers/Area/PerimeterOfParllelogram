import java.util.Scanner;

public class PerimeterOfParllelogram {
    
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.println("Enter the values :");
        int a = in.nextInt();
        int b = in.nextInt();
        int p = 2*(a+b);
        System.out.println("Perimeter of parllelogram is :" + (p) );
        
    }
}
